{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "isolatedModules": false,
    "allowJs": false,
    "noEmit": true,
    "outDir": "dist",
    "resolveJsonModule": true,
  }
}
